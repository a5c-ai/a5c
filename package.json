{
  "name": "@a5c-ai/events",
  "version": "1.1.0-a5c-main.11",
  "description": "Events SDK and CLI for normalizing and enriching repo/CI events for agentic workflows",
  "type": "module",
  "private": false,
  "bin": {
    "events": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "pretest": "npm run build",
    "prepare": "npm run build",
    "postinstall": "husky install || true",
    "clean": "rimraf dist .turbo .vitest coverage",
    "build": "tsc -p tsconfig.build.json && node scripts/build-post.js",
    "build:clean": "rimraf dist && npm run build",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" \"tests/**/*.ts\"",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "test": "vitest run",
    "test:ci": "vitest run --coverage --reporter=dot",
    "dev": "tsx watch src/cli.ts -- --help",
    "cli": "node dist/cli.js",
    "prepush:changed": "node scripts/prepush-related.js",
    "prepush:full": "vitest run",
    "prepush": "node scripts/prepush-related.js || vitest run",
    "release": "semantic-release"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a5c-ai/events.git"
  },
  "homepage": "https://github.com/a5c-ai/events",
  "bugs": {
    "url": "https://github.com/a5c-ai/events/issues"
  },
  "keywords": [
    "events",
    "cli",
    "sdk",
    "github",
    "normalization",
    "enrich"
  ],
  "author": "a5c.ai",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "9.35.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@types/node": "^22.7.4",
    "@types/yaml": "1.9.6",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vitest/coverage-v8": "^2.1.1",
    "ajv": "^8.17.1",
    "ajv-formats": "3.0.1",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-import": "^2.29.1",
    "globals": "16.4.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.2.0",
    "tsx": "^4.19.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0",
    "vitest": "^2.1.1",
    "yaml": "2.8.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "minimatch": "^9.0.5",
    "yaml": "^2.8.1",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "{test,tests}/**/*.{ts,tsx,js}": [
      "eslint --fix --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{md,json,yml,yaml}": [
      "prettier -w"
    ]
  }
}
