{
  "name": "@a5c/events",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "SDK/CLI to parse and enrich GitHub events for agentic workflows",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.json",
    "build:clean": "rimraf dist && npm run build",
    "lint": "eslint . --ext .ts",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "dev": "tsx watch src/cli.ts -- --help",
    "cli": "node dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": ["events", "cli", "sdk", "github", "normalization", "enrich"],
  "author": "a5c.ai",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.1",
    "typescript": "5.9.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.0",
    "minimatch": "^9.0.5"
  },
  "devDependencies": {}
}
